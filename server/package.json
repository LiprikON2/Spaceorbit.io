    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "prisma": "^4.12.0",
        "typescript": "^4.9.3",
        "vite": "^4.2.1",
        "vite-plugin-node": "^3.0.2"
    },
    "dependencies": {
        "@geckos.io/client": "^2.3.1",
        "@geckos.io/phaser-on-nodejs": "^1.2.8",
        "@geckos.io/server": "^2.3.1",
        "@prisma/client": "^4.12.0",
        "@spaceorbit/client": "file:../client",
        "@types/bcrypt": "^5.0.0",
        "@types/express-list-endpoints": "^6.0.0",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/nodemailer": "^6.4.7",
        "@types/uuid": "^9.0.1",
        "bcrypt": "^5.1.0",
        "express": "^4.18.2",
        "express-body-parser-error-handler": "^1.0.4",
        "express-list-endpoints": "^6.0.0",
        "jsonwebtoken": "^9.0.0",
