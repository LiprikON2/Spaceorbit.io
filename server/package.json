{
    "name": "@spaceorbit/server",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "prisma": {
        "schema": "src/db/schema.prisma"
    },
    "scripts": {
        "dev": "npm run prod",
        "prod": "npx nodemon --exec \"vite build && node dist/app.js\" -e ts --ignore dist/",
